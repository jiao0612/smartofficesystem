<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.itjzmproject.filesystem.dao.mapper.FileEntityMapper">

    <resultMap id="BaseResultMap" type="com.itjzmproject.filesystem.dao.entity.FileEntity">
            <id property="id" column="id" jdbcType="INTEGER"/>
            <result property="createTime" column="create_time" jdbcType="TIMESTAMP"/>
            <result property="updateTime" column="update_time" jdbcType="TIMESTAMP"/>
            <result property="fileName" column="file_name" jdbcType="VARCHAR"/>
            <result property="filePath" column="file_path" jdbcType="VARCHAR"/>
            <result property="operator" column="operator" jdbcType="VARCHAR"/>
            <result property="fileSize" column="file_size" jdbcType="VARCHAR"/>
    </resultMap>

    <sql id="Base_Column_List">
        id,create_time,update_time,
        file_name,file_path,operator,
        file_size
    </sql>
    <select id="list" resultType="com.itjzmproject.filesystem.dao.entity.FileEntity">
        select <include refid="Base_Column_List"/>
        from file_entity
    </select>
</mapper>
